{
  "name": "news-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.17.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:s3": "node scripts/dev-with-s3.js",
    "build": "next build",
    "build:staging": "NODE_ENV=production NEXT_PUBLIC_ENV=staging next build",
    "build:production": "NODE_ENV=production NEXT_PUBLIC_ENV=production next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "import-articles": "node scripts/import-articles.js",
    "copy-images": "node scripts/copy-images.js",
    "deploy:incremental": "tsx scripts/deploy-with-incremental-updates.ts",
    "deploy:staging": "npm run build:staging && tsx scripts/deploy-with-incremental-updates.ts --staging",
    "deploy:production": "npm run build:production && tsx scripts/deploy-with-incremental-updates.ts --production",
    "deploy:status": "tsx scripts/check-deployment-status.ts",
    "deploy:history": "tsx scripts/view-deployment-history.ts",
    "deploy:rollback": "tsx scripts/rollback-deployment.ts",
    "export-static": "next build && rm -rf ../itsquare-deployment/* && cp -r out/* ../itsquare-deployment && rm -rf out",
    "test:staging": "npm run lint && npm run build:staging",
    "test:production": "npm run lint && npm run build:production",
    "index:rebuild": "node scripts/rebuild-search-indexes.js",
    "index:setup-auto": "node scripts/setup-auto-indexing.js",
    "index:setup-policy": "node scripts/setup-s3-public-policy.js",
    "index:build-static": "node scripts/build-static-search-index.js",
    "index:upload-static": "node scripts/upload-static-search-index.js",
    "index:all-static": "npm run index:build-static && npm run index:upload-static",
    "search:test-encoding": "node scripts/test-search-encoding.js",
    "search:invalidate-cache": "node scripts/invalidate-search-cache.js",
    "search:deploy-fixes": "./scripts/deploy-search-fixes.sh",
    "env:check": "node scripts/check-environment.js",
    "env:align": "node scripts/align-environment.js"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.864.0",
    "@aws-sdk/client-cloudformation": "^3.864.0",
    "@aws-sdk/client-cloudfront": "^3.882.0",
    "@aws-sdk/client-dynamodb": "^3.864.0",
    "@aws-sdk/client-iam": "^3.864.0",
    "@aws-sdk/client-lambda": "^3.865.0",
    "@aws-sdk/client-personalize": "^3.882.0",
    "@aws-sdk/client-personalize-events": "^3.882.0",
    "@aws-sdk/client-personalize-runtime": "^3.882.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/credential-providers": "^3.864.0",
    "@aws-sdk/lib-dynamodb": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@react-google-maps/api": "^2.20.7",
    "@tailwindcss/postcss": "^4",
    "@types/nodemailer": "^6.4.17",
    "@vercel/mcp-adapter": "^1.0.0",
    "aws-amplify": "^6.15.4",
    "date-fns": "^4.1.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.11.17",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.15.1",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.468.0",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.14.0",
    "markdown-it": "^14.1.0",
    "mcp-handler": "^1.0.1",
    "next": "^15.5.1-canary.21",
    "nodemailer": "^7.0.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^4.12.0",
    "tailwindcss": "^4",
    "uuid": "^11.1.0",
    "yahoo-finance2": "^2.13.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@aws-amplify/adapter-nextjs": "^1.6.6",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.3",
    "@node-rs/jieba": "^1.6.0",
    "@types/lunr": "^2.3.7",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "archiver": "^6.0.2",
    "eslint": "^9",
    "eslint-config-next": "^15.4.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "module-alias": "^2.2.3",
    "node-fetch": "^3.3.2",
    "nodejieba": "^3.5.2",
    "puppeteer": "^24.16.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "strip-markdown": "^6.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "unified": "^11.0.5"
  }
}
