{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListContent2025",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::itsquareupdatedcontent",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "content/posts/2025/*"
          ]
        }
      }
    },
    {
      "Sid": "AllowGetContent2025",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::itsquareupdatedcontent/content/posts/2025/*"
    },
    {
      "Sid": "AllowGetImages2025",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::itsquareupdatedcontent/static/images/blog/2025/*"
    }
  ]
}
